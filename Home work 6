class Animal:
    def voice(self):
        pass

class Dog(Animal):
    def voice(self):
        print("Aw-aw")

class Cat(Animal):
    def voice(self):
        print("Meeeow")

class Fox(Animal):
    def voice(self):
        print("Rinrinningin")

dog = Dog()
cat = Cat()
fox = Fox()

dog.voice()
cat.voice()
fox.voice()
